{
  "name": "thereader",
  "version": "1.0.0",
  "description": "The ultimate reader for people with dyslexia",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently --kill-others --prefix \"[{name}]\" --names \"backend,frontend\" \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "install:all": "npm install && cd client && npm install",
    "postinstall": "cd client && npm install && npm run build",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.892.0",
    "@aws-sdk/lib-storage": "^3.891.0",
    "@elevenlabs/elevenlabs-js": "^2.15.0",
    "@google-cloud/storage": "^7.17.1",
    "@google-cloud/vision": "^5.3.3",
    "@neondatabase/serverless": "^1.0.1",
    "@replit/object-storage": "^1.0.0",
    "@uppy/aws-s3": "^5.0.1",
    "@uppy/core": "^5.0.2",
    "@uppy/dashboard": "^5.0.2",
    "@uppy/react": "^5.0.3",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "google-auth-library": "^10.3.0",
    "image-size": "^2.0.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "openai": "^5.21.0",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "sqlite3": "^5.1.7",
    "uuid": "^13.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
